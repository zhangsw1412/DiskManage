	 <div class="container">
			<form class="form-inline bs-example bs-example-form" role="form" id="selectForm" method="post" action={:U(GROUP_NAME."/Anli/search")} >
				<div class="row" >
					<select class="form-control" id="selectType" name="type">
						 <option value="title">案例名</option>
						 <option value="author">作者</option>
