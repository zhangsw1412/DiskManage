						 <option value="department">单位</option>
						 <option value="key_words">关键词</option>
					</select>
					<!-- <input type="hidden" name="type" id="selectTypeValue" value=""> -->
					<!-- <input type="hidden" name="first" value="1"> -->
					<input type="text" name="keyword" class="form-control" placeholder="请输入要查询的关键词" onkeydown="if(event.keyCode==13){submitSearch();}">
			</form>
				<input type="button" value="查询案例" class="btn btn-primary" onclick="submitSearch()">
				</div>
